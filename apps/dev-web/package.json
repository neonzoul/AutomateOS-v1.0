{
  "name": "@automateos/dev-web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:with-mock": "node scripts/dev-with-mock.cjs",
    "build": "next build",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "start": "next start",
    "lint:disabled": "echo 'Lint disabled temporarily for CI'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:smoke": "playwright test smoke-happy-path.spec.ts",
    "test:e2e:live": "playwright test --project=chromium-live --project=firefox-live",
    "test:e2e:mock": "cross-env USE_MOCK_GATEWAY=true playwright test --project=chromium-mock",
    "test:e2e:headed": "playwright test --headed",
    "mock-gateway": "node mock-gateway.cjs",
    "mock-gateway:start": "node mock-gateway.cjs 3001 &"
  },
  "dependencies": {
    "@automateos/workflow-schema": "workspace:^",
    "@hookform/resolvers": "^5.2.1",
    "@xyflow/react": "^12.8.4",
    "framer-motion": "^12.23.12",
    "nanoid": "^5.1.5",
    "next": "^15.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "zod": "^4.1.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "cors": "^2.8.5",
    "cross-env": "^10.0.0",
    "express": "^4.21.2",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "vitest": "^3.2.4"
  }
}
